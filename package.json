{
  "name": "docman",
  "version": "1.0.0",
  "description": "document management system",
  "main": "index.js",
  "scripts": {
    "build2": "webpack",
    "build": "rm -rf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./tests,./coverage --copy-files",
    "start2": "nodemon server.js --exec babel-node",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register ./server/tests/**/*.js ./client/test/**/*.js",
    "coverage": "NODE_ENV=test istanbul cover  _mocha -- ./server/tests/**/*.js --compilers js:babel-core/register ",
    "coveralls": "npm test && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "test-setup": "npm run migration",
    "db-clean": "NODE_ENV=test sequelize db:seed:undo:all && NODE_ENV=test sequelize db:migrate:undo:all",
    "migration": "NODE_ENV=test sequelize db:migrate",
    "seed": "NODE_ENV=test sequelize db:seed:all",
    "heroku-postbuild": "NODE_ENV=production && npm run build",
    "start": "node dist/server.js"
  },
